<UserControl x:Class="HOICK.UserControls.NationalFocusControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HOICK.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas>
        <Image Source="{Binding FocusIcon}" Canvas.Left="30"/>
        <Image Source="/Images/focus_can_start_bg.png" Canvas.Top="82" />
        <Label Content="{Binding FocusName}" Foreground="White" Canvas.Top="93" Width="164" Height="39" HorizontalContentAlignment="Center"/>
    </Canvas>
</UserControl>
